generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model ConversionHistory {
  id            String   @id @default(cuid())
  fromCurrency  String
  toCurrency    String
  amount        Float
  result        Float
  rate          Float
  createdAt     DateTime @default(now())
}
